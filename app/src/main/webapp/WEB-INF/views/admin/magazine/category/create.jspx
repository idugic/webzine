<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<div xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:field="urn:jsptagdir:/WEB-INF/tags/form/fields" xmlns:form="urn:jsptagdir:/WEB-INF/tags/form" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" version="2.0">
  <jsp:directive.page contentType="text/html;charset=UTF-8" />
  <jsp:output omit-xml-declaration="yes" />
  <form:create id="fc_category" modelAttribute="category" path="/admin/magazine/category">
    <field:select field="readerType" id="c_category_readertype" itemValue="id" items="${readerTypeList}" path="/admin/magazine/reader_type" required="true" />
    <field:input field="name" id="c_category_name" required="true" max="75" />
  </form:create>
</div>
