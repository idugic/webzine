<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<div xmlns:field="urn:jsptagdir:/WEB-INF/tags/form/fields" xmlns:form="urn:jsptagdir:/WEB-INF/tags/form" xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0">
  <jsp:directive.page contentType="text/html;charset=UTF-8" />
  <jsp:output omit-xml-declaration="yes" />
  <form:update id="fc_rs_id_webzine_domain_User" modelAttribute="userBacking" idField="backingId" path="/admin/user" multipart="true" render="${empty dependencies}" versionField="none">
    <field:input field="userName" id="c_rs_id_webzine_domain_User_userName" required="true" readonly="true" />
    <field:input field="password" id="c_rs_id_webzine_domain_User_password" required="true" readonly="true" render="false" />
    <field:input field="newPassword" id="c_rs_id_webzine_domain_User_newPassword" max="15" />
    <field:select field="roleId" id="c_rs_id_webzine_domain_User_roleId" itemValue="id" items="${role}" path="/admin/role" />
    <field:select field="statusId" id="c_rs_id_webzine_domain_User_statusId" itemValue="id" items="${userStatus}" path="/admin/user_status" />
    <field:input field="firstName" id="c_rs_id_webzine_domain_User_firstName" max="50" />
    <field:input field="lastName" id="c_rs_id_webzine_domain_User_lastName" max="50" />
    <field:datetime dateTimePattern="${user_birthdate_date_format}" field="birthdate" id="c_rs_id_webzine_domain_User_birthdate" />
    <field:file field="image" id="c_rs_id_webzine_domain_User_image" />
    <field:input field="email" id="c_rs_id_webzine_domain_Address_email" max="100" validationRegex="^.+\@.+\..+$" validationMessageCode="validation.user.email.invalid" />
    <field:input field="phone" id="c_rs_id_webzine_domain_Address_phone" max="50" />
    <field:input field="streetLine" id="c_rs_id_webzine_domain_Address_streetLine" max="100" />
    <field:input field="city" id="c_rs_id_webzine_domain_Address_city" max="50" />
    <field:input field="postalCode" id="c_rs_id_webzine_domain_Address_postalCode" max="15" />
    <field:input field="country" id="c_rs_id_webzine_domain_Address_country" max="50" />
    <field:input field="countryCode" id="c_rs_id_webzine_domain_Address_countryCode" max="15" />
    <field:input field="www" id="c_rs_id_webzine_domain_Address_www" max="250" />
  </form:update>
</div>
